{
  "name": "@my-workspace/source",
  "version": "0.0.0",
  "license": "MIT",
  "packageManager": "npm@10.9.2",
  "scripts": {
    "dev": "turbo dev",
    "dev:backend": "npm run prisma:generate && cd libs/database && npm run build && cd ../../apps/backend && npm run dev",
    "dev:mobile": "cd apps/mobile && npm run dev",
    "build": "turbo build",
    "build:backend": "cd apps/backend && npm run build",
    "build:mobile": "cd apps/mobile && npm run build",
    "start:backend": "cd apps/backend && npm run start",
    "start:mobile": "cd apps/mobile && npm run start",
    "lint": "turbo lint",
    "test": "turbo test",
    "prisma:generate": "cd libs/database && npm run prisma:generate",
    "prisma:db-push": "cd libs/database && npm run prisma:db-push",
    "prisma:migrate": "cd libs/database && npm run prisma:migrate",
    "prisma:studio": "prisma studio --schema=libs/database/prisma/schema.prisma",
    "prisma:seed": "tsx libs/database/prisma/seed.ts"
  },
  "prisma": {
    "seed": "tsx libs/database/prisma/seed.ts"
  },
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*"
  ],
  "dependencies": {
    "@nestjs/common": "^10.0.2",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.18",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "11.0.0",
    "@nestjs/testing": "^10.0.2",
    "@prisma/client": "5.13.0",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.9",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.31.0",
    "jest": "~29.7.0",
    "jest-environment-node": "^29.4.1",
    "prettier": "^2.6.2",
    "prisma": "5.13.0",
    "ts-jest": "29.4.5",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "turbo": "^2.6.1",
    "typescript": "~5.9.2"
  }
}
